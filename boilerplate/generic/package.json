{
    "name": "boilerplate",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "ts-build": "tsc --build",
        "tex-build": "node scripts/tex-build.js",
        "tex-postbuild": "node scripts/tex-postbuild.js",
        "tex-prebuild": "node scripts/tex-prebuild.js",
        "tex-watch": "node scripts/tex-watch.js",
        "prebuild": "npm run ts-build && npm run tex-prebuild",
        "build": "npm run tex-build",
        "watch": "npm run tex-watch",
        "postbuild": "npm run tex-postbuild",
        "prettier": "prettier --check ../..",
        "prettier-fix": "prettier --write ../..",
        "lint": "eslint ../.. --ext yaxm --max-warnings=0"
    },
    "author": "author",
    "license": "MIT",
    "devDependencies": {
        "@expo-google-fonts/arimo": "^0.2.2",
        "@expo-google-fonts/cousine": "^0.2.2",
        "@expo-google-fonts/tinos": "^0.2.2",
        "@md-to-latex/converter": "^2.0.0-beta.4",
        "@md-to-latex/yaxm-printer-latex": "^1.0.0-beta.0",
        "@md-to-latex/manager": "^2.0.0-beta.2",
        "@md-to-latex/title": "1.2.0-beta.0",
        "@types/node": "^18.6.2",
        "chokidar": "^3.5.3",
        "eslint": "^8.20.0",
        "eslint-plugin-yaxm": "^1.0.0-beta.0",
        "prettier": "^2.7.1",
        "typescript": "^4.7.4",
        "js-yaml": "^4.1.0"
    }
}
