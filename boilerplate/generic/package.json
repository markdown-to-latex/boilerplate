{
    "name": "boilerplate",
    "version": "1.0.1",
    "description": "",
    "scripts": {
        "ts-build": "tsc --build",
        "docx-prebuild": "node scripts/docx-prebuild.js",
        "tex-build": "node scripts/tex-build.js",
        "tex-postbuild": "node scripts/tex-postbuild.js",
        "tex-prebuild": "node scripts/tex-prebuild.js",
        "tex-watch": "node scripts/tex-watch.js",
        "prebuild": "npm run ts-build && npm run tex-prebuild",
        "build": "npm run tex-build",
        "watch": "npm run tex-watch",
        "postbuild": "npm run tex-postbuild",
        "prettier": "prettier --check .",
        "prettier-fix": "prettier --write .",
        "lint": "eslint . --ext yaxm --max-warnings=0"
