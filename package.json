{
    "name": "boilerplate",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "ts-build": "tsc --build",
        "tex-build": "node scripts/tex-build.js",
        "tex-postbuild": "node scripts/tex-postbuild.js",
        "tex-prebuild": "node scripts/tex-prebuild.js",
        "tex-watch": "node scripts/tex-watch.js",
        "prebuild": "npm run ts-build && npm run tex-prebuild",
        "build": "npm run tex-build",
        "watch": "npm run tex-watch",
        "postbuild": "npm run tex-postbuild",
        "prettier": "prettier --check .",
        "prettier-fix": "prettier --write .",
        "lint": "eslint . --ext yaxm --max-warnings=0"
    },
    "author": "author",
    "license": "MIT",
    "devDependencies": {
        "@expo-google-fonts/arimo": "^0.2.2",
        "@expo-google-fonts/cousine": "^0.2.2",
        "@expo-google-fonts/tinos": "^0.2.2",
        "@md-to-latex/converter": "file:../md-to-latex-converter",
        "@md-to-latex/manager": "file:../md-to-latex-manager",
        "@md-to-latex/title": "file:../md-to-latex-title",
        "@types/node": "^18.6.2",
        "chokidar": "^3.5.3",
        "eslint": "^8.20.0",
        "eslint-plugin-yaxm": "file:../md-to-latex-eslint-converter",
        "prettier": "^2.7.1",
        "typescript": "^4.7.4"
    }
